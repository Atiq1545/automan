                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                    <option value="1">1994</option>
                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                    <option value="1">1994</option>
                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                    <option value="1">1994</option>
                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                </select>
                                </div>
                            </div>

                            <div class="item">
                                <p>Vehicle Year (Reg.)</p>
                                <div class="">
                                <select class="form-control select2" style="width:100%;padding:9px 15px;">
                                    <option value="">Select your vehicle (Reg.) year</option>
                                    <option value="1">1994</option>
                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                    <option value="1">1994</option>
                                    <option value="2">1995</option>
                                    <option value="3">1996</option>
                                    <option value="4">1997</option>
                                    <option value="1">1994</option>
